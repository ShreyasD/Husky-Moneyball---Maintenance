    <validationRules>
        <fullName>Approval_Status_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(Status__c) &amp;&amp;
(ISPICKVAL(Status__c, &apos;Awaiting Approval&apos;) || ISPICKVAL(Status__c, &apos;Approved&apos;))</errorConditionFormula>
        <errorMessage>Cannot manually trigger Approval Workflow statuses (Awaiting Approval, Approved).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISCHANGED(Status__c),
(ISPICKVAL(Status__c, &apos;New&apos;) &amp;&amp; !ISNEW()) ||
(ISPICKVAL(Status__c, &apos;Started&apos;) &amp;&amp; (TEXT(PRIORVALUE(Status__c)) &lt;&gt; &apos;New&apos; &amp;&amp;  TEXT(PRIORVALUE(Status__c)) &lt;&gt; &apos;On Hold&apos;)) ||
(ISPICKVAL(Status__c, &apos;On Hold&apos;) &amp;&amp; (TEXT(PRIORVALUE(Status__c)) &lt;&gt; &apos;New&apos; &amp;&amp;  TEXT(PRIORVALUE(Status__c)) &lt;&gt; &apos;Started&apos; &amp;&amp; TEXT(PRIORVALUE(Status__c)) &lt;&gt; &apos;Awaiting Approval&apos;)) ||
(ISPICKVAL(Status__c, &apos;Cancelled&apos;) &amp;&amp; (TEXT(PRIORVALUE(Status__c)) &lt;&gt; &apos;New&apos; &amp;&amp;  TEXT(PRIORVALUE(Status__c)) &lt;&gt; &apos;Started&apos; &amp;&amp; TEXT(PRIORVALUE(Status__c)) &lt;&gt; &apos;On Hold&apos; &amp;&amp; TEXT(PRIORVALUE(Status__c)) &lt;&gt; &apos;Awaiting Approval&apos;)),
false)</errorConditionFormula>
        <errorMessage>Rig Program status should be: New (Once the notification is created), Started (after New or On Hold), On Hold (After New, Awaiting Approval or Started), Cancelled (After New, On Hold. Awaiting Approval or Started)</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Create_Service_Rig_Work_Order</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Service Rig Work Order</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

if(isAssociatedProductionEngineer() || isRole(&quot;Service_Rig_Planner&quot;)) {
    var releasedRecordTypeId = getReleasedServiceRigProgramRecordTypeId();

    var messageReturn = sforce.apex.execute(&quot;ServiceNotificationRequestGlobal&quot;,&quot;createServiceRigWorkOrder&quot;,
                        {serviceRigProgramId:&quot;{!HOG_Service_Rig_Program__c.Id}&quot;, 
                         notificationServicingFormId:&quot;{!HOG_Service_Rig_Program__c.HOG_Service_Request_Notification_Form_Id__c}&quot;,
                         serviceRigProgramRecordTypeId:releasedRecordTypeId}
                        );

    if(!messageReturn || /^\s*$/.test(messageReturn)) {
        alert(&quot;Well Servicing Work Order successfully created!!&quot;);
    } else {
        alert(&quot;Error: &quot; + messageReturn);
    }

    location.reload(true);
} else {
    alert(&quot;Only Production Engineer associated with this Service Rig Program or a Service Rig Planner are allowed to create a Service Rig Work Order.&quot;);
    location.reload(true);
}

function getReleasedServiceRigProgramRecordTypeId() {
    //Get Released Record Type
    result = sforce.connection.query(&quot;Select Id, Name, DeveloperName &quot; +
                                     &quot;From RecordType &quot; +
                                     &quot;Where SObjectType = &apos;HOG_Service_Rig_Program__c&apos; &quot; +
                                     &quot;And DeveloperName = &apos;Released_Service_Rig_Program&apos;&quot;);
    records = result.getArray(&quot;records&quot;);
    var releasedRecordType = records[0];

    return releasedRecordType.Id;
}

function isRole(roleDeveloperName) {
    var currentUser = sforce.connection.getUserInfo();

    var result = sforce.connection.query(&quot;Select Id, DeveloperName from UserRole Where DeveloperName = &apos;&quot; + roleDeveloperName + &quot;&apos;&quot;);
    var records = result.getArray(&quot;records&quot;);
    var role = records[0];

    return currentUser.roleId == role.Id;
}

function isAssociatedProductionEngineer() {
    var currentUser = sforce.connection.getUserInfo();
    return currentUser.userFullName == &quot;{!HOG_Service_Rig_Program__c.Production_Engineer__c}&quot;;
}</url>
