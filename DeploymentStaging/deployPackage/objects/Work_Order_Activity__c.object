        <fullName>Time_Spent_Off_Equipment_Text__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISNULL(Travel_Time__c ), &quot;&quot;,

IF(FLOOR(Travel_Time__c)=0, &quot;&quot;,
TEXT(
FLOOR(Travel_Time__c)
) &amp;&quot; Hours &quot;)

&amp;

IF(ROUND(MOD((Travel_Time__c)*60,60),0)=0, &quot;&quot;,
TEXT(
ROUND(MOD((Travel_Time__c)*60,60),0)
) &amp;&quot; Minutes &quot;)
)</formula>
