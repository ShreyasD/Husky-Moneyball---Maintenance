TEXT(
FLOOR(Work_Time__c)
) &amp;&quot; Hours &quot;)

&amp;

IF(ROUND(MOD((Work_Time__c )*60,60),0)=0, &quot;&quot;,
TEXT(
ROUND(MOD((Work_Time__c )*60,60),0)
) &amp;&quot; Minutes &quot;)
)</formula>
